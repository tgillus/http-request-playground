    "@typescript-eslint/parser": "5.54.1",
    "@vitest/coverage-c8": "0.29.2",
    "del-cli": "5.0.0",
    "eslint": "8.36.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "tsx": "3.12.3",
    "typescript": "4.9.5",
    "vitest": "0.29.2"
  },
  "dependencies": {
    "got": "12.6.0",
